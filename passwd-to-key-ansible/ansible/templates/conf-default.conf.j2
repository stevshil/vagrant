server {
    listen       {{ nginxport }} default_server;
    listen       [::]:{{nginxport}} default_server;
    server_name  _;
    root         {{ docroot }};

    # Load configuration files for the default server block.
    include /etc/nginx/default.d/*.conf;

    location / {
    }

    error_page 404 /404.html;
        location = /40x.html {
    }

    error_page 500 502 503 504 /50x.html;
        location = /50x.html {
    }
}

